class Solution {
    public int searchInsert(int[] arr, int target) {
      int len = arr.length;
      int start=0,end=len-1;
			while(start<end) {
			int mid = start+(end-start)/2;
			if(arr[mid]<target) {
				start = mid+1;
			}
			else if(arr[mid]>target) {
				end = mid;
			}
			else return mid;
		}
			if(arr[start]<target) return start+1;
		return start;
    }
}
